{
  "name": "@dweidner/eleventy-starter-kit",
  "version": "0.1.0",
  "description": "An opinionated starter kit for Eleventy",
  "repository": "dweidner/eleventy-starter-kit",
  "author": "Daniel Weidner <hallo@danielweidner.de>",
  "license": "ISC",
  "browser": "dist/assets/scripts/main.legacy.js",
  "module": "dist/assets/scripts/main.modern.js",
  "style": " dist/assets/styles/main.css",
  "private": true,
  "keywords": [
    "babel",
    "eleventy",
    "eslint",
    "postcss",
    "rollup",
    "stylelint"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "browserslist": {
    "modern": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ],
    "legacy": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR",
      "not dead"
    ]
  },
  "scripts": {
    "start": "run-s --silent serve",
    "clean": "rimraf dist",
    "build": "run-s --silent clean build:production",
    "build:development": "eleventy",
    "build:production": "cross-env NODE_ENV=production eleventy",
    "serve": "eleventy --serve",
    "lint": "run-s --silent lint:*",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint 'src/assets/styles/**/*.css'"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "autoprefixer": "^10.4.13",
    "core-js": "^3.27.2",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.14",
    "dotenv": "^16.0.3",
    "dynamic-import-polyfill": "^0.1.1",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-calc": "^8.2.4",
    "postcss-color-functional-notation": "^4.2.4",
    "postcss-custom-media": "^8.0.2",
    "postcss-custom-properties": "^12.1.11",
    "postcss-gap-properties": "^3.0.3",
    "postcss-import": "^15.1.0",
    "postcss-inset": "^1.0.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nesting": "^10.1.4",
    "postcss-url": "^10.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.71.1",
    "rollup-plugin-terser": "^7.0.2",
    "slugify": "^1.6.5",
    "whatwg-fetch": "^3.6.2",
    "wicked-elements": "^3.1.2"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "stylelint": "^14.8.1",
    "stylelint-config-standard": "^25.0.0"
  }
}
