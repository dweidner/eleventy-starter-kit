{
  "name": "@dweidner/eleventy-starter-kit",
  "version": "0.2.0",
  "description": "An opinionated starter kit for Eleventy",
  "repository": "dweidner/eleventy-starter-kit",
  "author": "Daniel Weidner <hallo@danielweidner.de>",
  "license": "ISC",
  "private": true,
  "keywords": [
    "eleventy",
    "vite",
    "postcss",
    "eslint",
    "stylelint"
  ],
  "engines": {
    "node": "^14.13.1 || >=16"
  },
  "browserslist": [
    "defaults",
    "supports css-variables",
    "supports es6-module",
    "supports es6-module-dynamic-import"
  ],
  "scripts": {
    "start": "run-s --silent serve",
    "clean": "rimraf dist",
    "build": "run-s --silent clean build:production",
    "build:development": "cross-env NODE_ENV=development eleventy",
    "build:production": "cross-env NODE_ENV=production eleventy",
    "serve": "cross-env NODE_ENV=development eleventy --serve",
    "lint": "run-s --silent lint:*",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint 'src/assets/styles/**/*.css'",
    "format": "run-s --silent format:*",
    "format:scripts": "eslint --fix .",
    "format:styles": "stylelint --fix 'src/assets/styles/**/*.css'"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.0-canary.15",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-vite": "^2.0.0-canary.2",
    "autoprefixer": "^10.4.9",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-calc": "^8.2.4",
    "postcss-color-functional-notation": "^4.2.4",
    "postcss-custom-media": "^8.0.2",
    "postcss-gap-properties": "^3.0.5",
    "postcss-import": "^15.0.0",
    "postcss-inset": "^1.0.0",
    "postcss-nesting": "^10.1.10",
    "postcss-url": "^10.1.3",
    "rimraf": "^3.0.2",
    "slugify": "^1.6.5",
    "wicked-elements": "^3.1.2"
  },
  "devDependencies": {
    "eslint": "^8.23.1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "stylelint": "^14.11.0",
    "stylelint-config-standard": "^28.0.0"
  }
}
